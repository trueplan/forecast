{
  "name": "@trueplan/forecast-theme",
  "version": "3.0.0",
  "description": "Forecast Design System Theme",
  "author": "TruePlan Inc.",
  "license": "UNLICENSED",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/trueplan/forecast",
    "directory": "packages/theme"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "rollup -c node:forecast --environment NODE_ENV:production",
    "compile:dev": "rollup -c node:forecast --environment NODE_ENV:development",
    "type-check": "tsc --noEmit --composite false"
  },
  "dependencies": {
    "@stitches/react": "^1.2.7",
    "react-uid": "^2.3.2"
  },
  "peerDependencies": {
    "@stitches/react": "^1.2.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-uid": "^2.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@stitches/react": "^1.2.7",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.4",
    "@types/react-dom": "^18.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-uid": "^2.3.2",
    "rollup": "^2.70.1",
    "rollup-config-forecast": "workspace:configs/rollup",
    "tsconfig-forecast": "workspace:configs/tsconfig",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
