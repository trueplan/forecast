{
  "name": "@trueplan/forecast-icons",
  "version": "4.0.3",
  "description": "Forecast Design System Icons",
  "author": "TruePlan Inc.",
  "license": "UNLICENSED",
  "source": "index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/trueplan/forecast",
    "directory": "packages/icons"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "rollup -c node:forecast --environment NODE_ENV:production",
    "compile:dev": "rollup -c node:forecast --environment NODE_ENV:development",
    "type-check": "tsc --noEmit --composite false",
    "convert:icons": "node ./tools/cli.js convert",
    "convert:icons-new": "node ./tools/cli.js convert-new"
  },
  "dependencies": {
    "lodash.difference": "4.5.0",
    "lodash.startcase": "4.4.0"
  },
  "peerDependencies": {
    "@trueplan/forecast-components": "^4.0.0",
    "@trueplan/forecast-theme": "^3.0.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-uid": "2.x"
  },
  "devDependencies": {
    "@svgr/babel-plugin-add-jsx-attribute": "6.0.0",
    "@svgr/babel-plugin-remove-jsx-attribute": "6.0.0",
    "@svgr/plugin-svgo": "6.2.0",
    "@trueplan/forecast-components": "4.7.6",
    "@trueplan/forecast-theme": "3.2.1",
    "@twilio-labs/svg-to-react": "2.1.1",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "ansi-styles": "6.1.0",
    "chalk": "5.0.1",
    "commander": "9.3.0",
    "lodash.difference": "4.5.0",
    "lodash.startcase": "4.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-uid": "2.3.2",
    "rollup": "2.75.7",
    "rollup-config-forecast": "workspace:configs/rollup",
    "tsconfig-forecast": "workspace:configs/tsconfig",
    "tslib": "2.4.0",
    "typescript": "4.4.3"
  }
}
