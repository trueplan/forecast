  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/trueplan/forecast",
    "directory": "packages/icons"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "rollup -c node:forecast --environment NODE_ENV:production",
    "compile:dev": "rollup -c node:forecast --environment NODE_ENV:development",
    "type-check": "tsc --noEmit --composite false",
    "publish": "yarn npm publish --tolerate-republish",
    "convert:icons": "node ./tools/cli.js convert",
    "convert:icons-new": "node ./tools/cli.js convert-new"
  },
